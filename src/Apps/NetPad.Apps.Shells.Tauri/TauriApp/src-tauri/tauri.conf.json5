{
    "productName": "NetPad",
    "version": "0.8.0",
    "identifier": "com.netpad.app",
    "build": {
        "beforeDevCommand": {
            "script": "npm run start-web",
            "cwd": "../../../NetPad.Apps.App/App"
        },
        "devUrl": "http://localhost:57940?shell=tauri",
        "frontendDist": "./loader"
    },
    "app": {
        "windows": [
            {
                "label": "main",
                "title": "NetPad",
                "maximized": true,
                "width": 1200,
                "height": 800,
                "resizable": true,
                "fullscreen": false,
                "decorations": false
            }
        ],
        "security": {
            "csp": null,
            "capabilities": [
                "default"
            ]
        }
    },
    "bundle": {
        "active": true,
        "targets": "all",
        "icon": [
            "icons/32x32.png",
            "icons/128x128.png",
            "icons/128x128@2x.png",
            "icons/icon.icns",
            "icons/icon.ico"
        ]
    },
    "$schema": "../node_modules/@tauri-apps/cli/config.schema.json"
}
